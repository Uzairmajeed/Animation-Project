<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Objectactivity">


    <soup.neumorphism.NeumorphImageButton
        android:id="@+id/neumorphImageButtonCar"
        android:layout_width="250dp"
        android:layout_height="200dp"
        android:layout_marginTop="136dp"
        android:src="@drawable/baseline_directions_car_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <soup.neumorphism.NeumorphImageButton
        android:id="@+id/neumorphImageButtonBike"
        android:layout_width="250dp"
        android:layout_height="200dp"
        android:src="@drawable/baseline_directions_bike_24"
       app:layout_constraintBottom_toTopOf="@id/nextButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/neumorphImageButtonCar"
        app:layout_constraintVertical_bias="0.189" />

    <soup.neumorphism.NeumorphImageButton
        android:id="@+id/nextButton"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="72dp"
        android:src="@drawable/baseline_navigate_next_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/neumorphImageButtonBike" />

</androidx.constraintlayout.widget.ConstraintLayout>